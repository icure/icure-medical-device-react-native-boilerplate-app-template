// prettier-ignore
{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",

    /* Completeness */
    "skipLibCheck": true,
    "types": [ "node" ],
    "strictNullChecks": false,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "sourceMap": true
  },
  
}
